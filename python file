   "source": [
    "# 1. Count the number of patients by blood type\n",
    "blood_type_counts = df['Blood Type'].value_counts()\n",
    "\n",
    "# 2. Calculate the percentage for each blood type\n",
    "blood_type_percentages = (blood_type_counts / blood_type_counts.sum()) * 100\n",
    "\n",
    "# 3. Display total number of patients\n",
    "total_patients = blood_type_counts.sum()\n",
    "print(f\"Total number of patients: {total_patients}\")\n",
    "print(blood_type_counts)\n",
    "print(blood_type_percentages)\n",
    "\n",
    "# 4. Visualize the blood type distribution using a donut chart\n",
    "plt.figure(figsize=(8, 8))\n",
    "# Create a pie chart first\n",
    "wedges, texts, autotexts = plt.pie(\n",
    "    blood_type_counts, \n",
    "    labels=blood_type_counts.index, \n",
    "    autopct='%1.1f%%', \n",
    "    startangle=90, \n",
    "    colors=plt.cm.Paired.colors, \n",
    "    wedgeprops=dict(width=0.3, edgecolor='w'),\n",
    "    pctdistance=0.85\n",
    ")\n",
    "\n",
    "# Add the count of patients as labels on the donut chart\n",
    "for i, text in enumerate(autotexts):\n",
    "    text.set_text(f'{blood_type_counts[i]} ({blood_type_percentages[i]:.1f}%)')\n",
    "\n",
    "# Add a white circle in the center to create the \"donut\" effect\n",
    "centre_circle = plt.Circle((0,0),0.70,fc='white')\n",
    "fig = plt.gcf()\n",
    "fig.gca().add_artist(centre_circle)\n",
    "\n",
    "# Set the title\n",
    "plt.title(f'Blood Type Distribution of Patients (Total: {total_patients})')\n",
    "\n",
    "# Display the chart\n",
    "plt.show()"
   ]
  },
